{
  "name": "course-omnifood",
  "version": "1.0.0",
  "description": "Udemy 'Build Responsive Real World Websites with HTML5 and CSS3' course",
  "repository": {
    "type": "git",
    "url": "https://github.com/RubenSantana/course-omnifood"
  },
  "dependencies": {
    "jquery": "^3.4.1",
    "normalize.css": "^8.0.1",
    "waypoints": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "autoprefixer": "7.1.6",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-preset-es2015": "^6.24.1",
    "browser-sync": "^2.26.7",
    "eslint": "^5.16.0",
    "gulp": "4.0.2",
    "gulp-postcss": "7.0.0",
    "gulp-watch": "4.3.11",
    "husky": "^2.4.1",
    "postcss-import": "^12.0.1",
    "postcss-mixins": "^6.2.1",
    "postcss-nested": "^2.1.2",
    "postcss-preset-env": "^6.6.0",
    "postcss-simple-vars": "4.1.0",
    "prettier": "1.18.2",
    "pretty-quick": "^1.11.1",
    "webpack": "^4.34.0"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jest": true
    },
    "rules": {
      "no-debugger": "off",
      "no-console": "off",
      "no-unused-vars": "warn"
    },
    "root": true
  }
}
